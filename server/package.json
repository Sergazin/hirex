{
  "name": "server",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "start": "env $(cat .env) npx tsx ./src/main.ts",
    "dev": "env $(cat .env) nodemon -x 'npx tsx ./src/main.ts || touch ./src/main.ts' -w src -e ts",
    "dev:x": "env $(cat .env) nodemon -x 'node -r ts-node/register/transpile-only -r tsconfig-paths/register' -w src src/main.ts",
    "dev:bun": "bun --watch src/main.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "bun-types": "latest",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@openai/realtime-api-beta": "openai/openai-realtime-api-beta",
    "audio-decode": "^2.2.2",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "date-fns": "^3.6.0",
    "express": "^4.21.1",
    "express-http-proxy": "^2.1.1",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.7.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.68.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  }
}
